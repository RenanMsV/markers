<PropertyList>
	<path>../models/checkpoint.ac</path>
	<animation>
		<type>billboard</type>
		<object-name>rect</object-name>
		<object-name>NameOsg</object-name>
		<spherical type="bool">false</spherical>
	</animation>
	<animation>
		<type>material</type>
		<object-name>rect</object-name>
		<object-name>NameOsg</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
	</animation>
	<animation>
	<animation>
  		<type>noshadow</type>
  		<object-name>rect</object-name>
  		<object-name>NameOsg</object-name>
 	</animation>
  	<type>range</type>
  		<min-m>0</min-m>
  		<max-m>10000</max-m>
 	</animation>
	<animation>
  		<type>scale</type>
		<object-name>rect</object-name>
		<x-offset>30</x-offset>
		<y-offset>1</y-offset>
		<z-offset>30</z-offset>
 	</animation>
	<animation>
		<type>dist-scale</type>
		<object-name>NameOsg</object-name>
		<object-name>rect</object-name>
		<interpolation>
			<entry>
				<ind>50</ind>
				<dep>1</dep>
			</entry>
			<entry>
				<ind>300</ind>
				<dep>1.5</dep>
			</entry>
			<entry>
				<ind>1500</ind>
				<dep>12</dep>
			</entry>
			<entry>
				<ind>5000</ind>
				<dep>24</dep>
			</entry>
			<entry>
				<ind>10000</ind>
				<dep>48</dep>
			</entry>
		</interpolation>
	</animation>
	<text>
		<name>NameOsg</name>
		<offsets>
			<pitch-deg>0</pitch-deg>
			<heading-deg>0</heading-deg>
			<roll-deg>0</roll-deg>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>15</z-m>
		</offsets>
	<font>Helvetica.txf</font>
    <axis-alignment>xz-plane</axis-alignment>
    <type type="string">literal</type>
    <text type="string">Estadio do Pacaembu</text>
    <font-resolution>
    	<width type="int">128</width>
    	<height type="int">128</height>
    </font-resolution>
    <layout>left-to-right</layout>
	<draw-text type="bool">true</draw-text>
	<draw-alignment type="bool">false</draw-alignment>
	<draw-boundingbox type="bool">false</draw-boundingbox>
	<character-size type="double">2</character-size>
	<character-aspect-ratio type="double">1.0</character-aspect-ratio>
	<max-height>25</max-height>
	<max-width>100</max-width> 
	<font-resolution>
		<width type="int">32</width>
		<height type="int">32</height>
	</font-resolution>
	<kerning>default</kerning>
	<alignment>center-center</alignment>
	</text>
</PropertyList>